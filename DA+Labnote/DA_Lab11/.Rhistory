data_gen <- function(N){
X1 = rnorm(N)
X2 = rnorm(N)
X3 = rnorm(N)
Z = 5 + 1 * (X1 + X2 + X3) + rnorm(N, sd = 4)
Y = 3 + 15 * (X1 - X2 + 2 * X3) + (Z - 5) ** 2 * I(Z > 5) + rnorm(N, sd = 1)
data = data.frame(X1, X2, X3, Y, Z)
return (data)
}
